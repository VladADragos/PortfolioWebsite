    "@types/react": "17.0.38",
    "autoprefixer": "^10.4.0",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "postcss": "^8.4.5",
    "prettier": "^2.6.0",
    "prettier-plugin-tailwindcss": "^0.1.1",
    "tailwindcss": "^3.0.7",
    "typescript": "4.5.4"
